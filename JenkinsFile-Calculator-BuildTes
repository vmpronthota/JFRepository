pipeline {
    agent any
    stages {
         stage("Checkout Stage") {
              steps {
                   git url: 'https://github.com/vmpronthota/calculator.git'
              }
         }
         stage("Compile Build Stage") {
              steps {
                   sh "./gradlew build"
              }
         }
         stage("Compile Test Stage") {
              steps {
                   sh "./gradlew test"
              }
         }
    }
}
